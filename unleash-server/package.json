{
    "name": "unleash-server",
    "description": "unleash your features",
    "version": "0.0.1",
    "keywords": [
        "unleash",
        "feature toggle",
        "feature",
        "toggle"
    ],
    "repository": {
        "type": "git",
        "url": "ssh://git@github.com:finn-no/unleash.git"
    },
    "bugs": {
        "url": "https://github.com/finn-no/unleash/issues"
    },
    "private": true,
    "scripts": {
        "start": "node server.js",
        "start-dev": "NODE_ENV=local supervisor --ignore ./node_modules/ server.js",
        "test": "jshint server.js lib public/scripts test && mocha test test/*",
        "tdd": "mocha --watch test test/*",
        "test-bamboo-ci": "mocha test test/*"
    },
    "dependencies": {
        "bluebird": "2.2.2",
        "body-parser": "1.4.3",
        "errorhandler": "1.1.1",
        "express": "4.9.8",
        "express-validator": "2.6.0",
        "ini": "1.3.0",
        "log4js": "0.6.21",
        "nconf": "0.6.9"
    },
    "devDependencies": {
        "chai": "1.9.1",
        "jshint": "2.5.2",
        "mocha": "1.20.1",
        "supertest": "0.13.0",
        "supervisor": "~0.6.0"
    }
}
